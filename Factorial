/*Realice un programa que calcule el factorial de un n√∫mero entero sin signo*/
#include <iostream>
using namespace std;

int factorial(int n){
    int p =1;
    for(int i=1;i<=n;i++){
      p=p*i;        
    }
    return p;
}

int main(){
    int x, y;
    cout<<"Ingrese el factorial"<<endl;
    cin>>x;
    int f = factorial(x);
    cout<<f;
    system("pause");
}
